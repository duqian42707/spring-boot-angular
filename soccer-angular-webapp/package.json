{
  "name": "portal",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run ng-high-memory build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "ng-high-memory": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng",
    "hmr": "ng s -o --hmr",
    "analyze": "npm run ng-high-memory build -- --source-map",
    "analyze:view": "source-map-explorer dist/**/*.js",
    "test-coverage": "ng test --code-coverage --watch=false",
    "color-less": "ng-alain-plugin-theme -t=colorLess",
    "theme": "ng-alain-plugin-theme -t=themeCss",
    "icon": "ng g ng-alain:plugin icon",
    "lint": "npm run lint:ts && npm run lint:style",
    "lint:ts": "ng lint --fix",
    "lint:style": "npx stylelint \"src/**/*.less\" --fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.12",
    "@angular/common": "^14.2.12",
    "@angular/compiler": "^14.2.12",
    "@angular/core": "^14.2.12",
    "@angular/forms": "^14.2.12",
    "@angular/platform-browser": "^14.2.12",
    "@angular/platform-browser-dynamic": "^14.2.12",
    "@angular/router": "^14.2.12",
    "@delon/abc": "^14.3.0",
    "@delon/acl": "^14.3.0",
    "@delon/auth": "^14.3.0",
    "@delon/cache": "^14.3.0",
    "@delon/chart": "^14.3.0",
    "@delon/form": "^14.3.0",
    "@delon/mock": "^14.3.0",
    "@delon/theme": "^14.3.0",
    "@delon/util": "^14.3.0",
    "ajv": "^8.10.0",
    "ajv-formats": "^2.1.1",
    "ng-alain": "^14.3.0",
    "ng-zorro-antd": "^14.0.0",
    "rxjs": "~7.5.0",
    "screenfull": "^6.0.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.10",
    "@angular-eslint/builder": "~14.0.3",
    "@angular-eslint/eslint-plugin": "~14.0.3",
    "@angular-eslint/eslint-plugin-template": "~14.0.3",
    "@angular-eslint/schematics": "~14.0.3",
    "@angular-eslint/template-parser": "~14.0.3",
    "@angular/cli": "^14.2.10",
    "@angular/compiler-cli": "^14.2.12",
    "@angular/language-service": "^14.2.0",
    "@delon/testing": "^14.3.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "~5.35.1",
    "@typescript-eslint/parser": "~5.35.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "~8.5.0",
    "eslint-plugin-import": "~2.26.0",
    "eslint-plugin-jsdoc": "~39.3.6",
    "eslint-plugin-prefer-arrow": "~1.2.3",
    "eslint-plugin-prettier": "~4.2.1",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lint-staged": "^12.3.7",
    "ng-alain-plugin-theme": "^14.0.0",
    "prettier": "^2.7.1",
    "source-map-explorer": "^2.5.2",
    "stylelint": "^14.6.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "stylelint-order": "^5.0.0",
    "typescript": "~4.6.2",
    "eslint-plugin-deprecation": "~1.3.2",
    "husky": "^7.0.4",
    "ng-alain": "^14.3.0"
  },
  "lint-staged": {
    "(src)/**/*.{html,ts}": [
      "eslint --fix"
    ],
    "(src)/**/*.less": [
      "npm run lint:style"
    ]
  }
}
